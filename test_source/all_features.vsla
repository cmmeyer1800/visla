canvas{
    width = 300;
    height = 300;
    line {
        width = 1;
        start = (0,0);
        end = (100, 100);
        color = (0, 255, 255);
    };
    line {
        start = (0,50);
        end = (100, 50);
        color = (0, 255, 0);
    };
    line {
        start = (50,0);
        end = (50, 100);
        color = (255, 0, 0);
    };

    line {
        width = 1;
        start = (0,100);
        end = (100, 0);
        color = (255, 255, 0);    
    };
    rect {
        center = (50,50);
        width = 100;
        height = 100;
        color = (255, 255, 255);
    };
    ellipse {
        center = (50,50);
        width = 100;
        height = 100;
        color = (255, 255, 255);
    };
    ellipse {
        center = (50,50);
        width = 50;
        height = 50;
        color = (255, 255, 255);
    };
    ellipse {
        center = (50,50);
        width = 10;
        height = 10;
        color = (255, 255, 255);
    };
};